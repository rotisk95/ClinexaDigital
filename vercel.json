{
  "buildCommand": "vite build",
  "outputDirectory": "dist/public",
  "framework": "vite",
  "functions": {
    "api/contact.js": {
      "runtime": "@vercel/node@2.15.10"
    }
  },
  "engines": {
    "node": "18.x"
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    }
  ]
}
